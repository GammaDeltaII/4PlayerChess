}
QPushButton:hover {
    	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 rgb(200, 200, 200), stop: 0.4 rgb(185, 185, 185), stop: 0.5 rgb(175, 175, 175), stop: 1.0 rgb(160, 160, 160));
}
QPushButton:pressed {
    	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 rgb(220, 220, 220), stop: 0.4 rgb(205, 205, 205), stop: 0.5 rgb(195, 195, 195), stop: 1.0 rgb(180, 180, 180));
}
QTabWidget::pane {
    border-top: 1px solid rgb(150, 150, 150);
}
QTabWidget::tab-bar {
    left: 10px;
}
QTabBar::tab {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 rgb(180, 180, 180), stop: 0.4 rgb(165, 165, 165), stop: 0.5 rgb(155, 155, 155), stop: 1.0 rgb(140, 140, 140));
    border: 1px solid rgb(150, 150, 150);
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 10ex;
    padding: 2px;
}
QTabBar::tab:selected, QTabBar::tab:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 rgb(220, 220, 220), stop: 0.4 rgb(205, 205, 205), stop: 0.5 rgb(195, 195, 195), stop: 1.0 rgb(180, 180, 180));
}
QTabBar::tab:!selected {
    margin-top: 2px;
}
QTabBar::tab:selected {
    margin-left: -4px;
    margin-right: -4px;
}
QTabBar::tab:first:selected {
    margin-left: 0;
}
QTabBar::tab:last:selected {
    margin-right: 0;
}
QTabBar::tab:only-one {
    margin: 0;
}</string>
